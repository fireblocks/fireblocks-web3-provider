{
  "name": "@fireblocks/fireblocks-web3-provider",
  "version": "1.0.0",
  "description": "EIP-1193 Compatible Ethereum provider for Fireblocks",
  "repository": "github:fireblocks/fireblocks-web3-provider",
  "author": "Fireblocks",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "wallet",
    "fireblocks",
    "jsonrpc",
    "web3",
    "crypto",
    "cryptocurrency",
    "dapp"
  ],
  "scripts": {
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "mocha --exit --exclude 'test/gasless/*.test.ts' --recursive 'test/**/*.test.ts'",
    "test:gasless": "mocha --exit --recursive 'test/gasless/*.test.ts'",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/node-fetch": "^2.6.2",
    "chai": "4.3.6",
    "ethers": "5.6.9",
    "mocha": "10.0.0",
    "ts-node": "10.8.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.7.4",
    "web3": "1.7.4"
  },
  "dependencies": {
    "@ethersproject/units": "^5.7.0",
    "@json-rpc-tools/utils": "1.7.5",
    "dotenv": "^16.0.3",
    "fireblocks-sdk": "2.5.4",
    "node-fetch": "^2.6.7",
    "web3-providers-http": "1.8.0"
  }
}
